<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane maxHeight="312.0" maxWidth="499.0" minHeight="262.0" minWidth="476.0" prefHeight="312.0" prefWidth="499.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pjatk.pcwieka.gui.project.snakegame.application.controller.MainController">
   <center>
      <VBox alignment="CENTER" prefHeight="282.0" prefWidth="488.0" spacing="30.0" styleClass="background" stylesheets="@../css/general.css" BorderPane.alignment="CENTER">
         <children>
            <Text fx:id="gameNameText" fill="WHITE" smooth="false" styleClass="snake-text" text="Snake The Game">
               <font>
                  <Font size="12.0" />
               </font></Text>
            <Button fx:id="newGameButton" alignment="CENTER" mnemonicParsing="false" onAction="#handleNewGameButtonAction" styleClass="snake-button" text="New Game" />
            <Button fx:id="highScoreButton" alignment="CENTER" mnemonicParsing="false" onAction="#handleHighScoreButtonAction" styleClass="snake-button" text="High Score" />
            <Button fx:id="exitButton" alignment="CENTER" mnemonicParsing="false" onAction="#handleExitButtonAction" prefWidth="86.0" styleClass="snake-button" text="Exit" textAlignment="CENTER" />
         </children>
         <padding>
            <Insets top="10.0" />
         </padding>
      </VBox>
   </center>
</BorderPane>
